<Window x:Class="datepicker_of_wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:datepicker_of_wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DatePicker  Foreground="SlateGray" SelectedDateChanged="DatePicker_SelectedDateChanged" SelectedDateFormat="Long" >
            <DatePicker.BlackoutDates>
                <CalendarDateRange Start="02/10/2021" End="10/12/2021"></CalendarDateRange>
            </DatePicker.BlackoutDates>
        </DatePicker>
    </Grid>
</Window>
